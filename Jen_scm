pipeline {
    agent any 
    stages {
        stage('melluzzoo') { 
            steps {
               sh " pwd"
            }
        }
        stage('BRANCH ......') { 
            steps {
               sh " echo THIS IS A BRANCH ! !!"
               sh "pwd"
               
            }
        }
        stage('MOUUUSE') { 
            steps {
                sh " pwd"
               sh "echo Mouse-----end"
            }
        }
    }
}
